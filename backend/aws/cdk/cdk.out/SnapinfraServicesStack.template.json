    "VersioningConfiguration": {
     "Status": "Enabled"
    }
   },
   "UpdateReplacePolicy": "Retain",
   "DeletionPolicy": "Retain",
   "Metadata": {
    "aws:cdk:path": "SnapinfraServicesStack/SnapinfraStorageBucket/Resource"
   }
  },
  "DeadLetterQueue9F481546": {
