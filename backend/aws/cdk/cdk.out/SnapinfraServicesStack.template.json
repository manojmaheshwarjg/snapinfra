{
 "Description": "Snapinfra S3, SQS, and SNS Infrastructure",
 "Resources": {
  "SnapinfraStorageBucketE29A0E01": {
   "Type": "AWS::S3::Bucket",
   "Properties": {
    "BucketEncryption": {
